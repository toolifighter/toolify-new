{
  "expo": {
    "name": "Toolify",
    "description": "An App for everything.",
    "slug": "toolify",
    "privacy": "public",
    "sdkVersion": "30.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ericjkl.toolifyapp"
    },

    "android": {
      "package": "com.ericjkl.toolifyapp"
    },
    
    
    "githubUrl": "https://github.com/ericjkl/toolapp"
  }
}